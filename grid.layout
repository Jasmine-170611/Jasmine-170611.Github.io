<DOCTYPE html>
  head
      body {
              <div class="container">
              <div class="cell cell-1">1.</div>
              <div class"cell cell-2">2.</div>
              <div class"cell cell-3">3.</div>
              <div class"cell cell-4">4.</div>
              <div class"cell cell-5">5.</div>
              <div class"cell cell-6">6.</div>
              <div class"cell cell-7">7.</div>
          </div>
 .container{
        height: 70vh;
         margin: 1rem;
         display: grid;
         grid-template-columns: 2fr 1fr 4fr;
         grid-template-rows: 1fr 1fr 1fr;
}
 .cell{
      color: white;
      front-size: 4rem;
      text-align: center;
      padding: 5rem;
}
.cell-1{
    outline: skyblue;
}
.cell-2{
     outline: green;
}
.cell-3{
     outline: red;
}
.cell-4{
     outline: purple;
}
.cell-5{
    outline: grey;
}
.cell-6{
     outline: orange;
}
.cell-7{
      outline: brown;
}
/head
/body
    
